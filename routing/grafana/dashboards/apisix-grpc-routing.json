{
  "dashboard": {
    "title": "APISIX gRPC Routing Dashboard",
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "10s",
    "panels": [
      {
        "id": 1,
        "title": "Total Requests by Server",
        "type": "graph",
        "targets": [
          {
            "expr": "sum by (service) (rate(grpc_requests_total[1m]))",
            "legendFormat": "{{service}}"
          }
        ],
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8}
      },
      {
        "id": 2,
        "title": "Request Duration",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(grpc_request_duration_seconds_bucket[1m])) by (le, service))",
            "legendFormat": "{{service}} p95"
          }
        ],
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8}
      }
    ]
  }
}
