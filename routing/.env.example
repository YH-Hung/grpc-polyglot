# Environment variables for gRPC Routing Demo
# Copy this file to .env and customize as needed

# APISIX Configuration
APISIX_LISTEN_PORT=9080
APISIX_ADMIN_PORT=9180
APISIX_METRICS_PORT=9091

# gRPC Servers
GO_SERVER_PORT=50051
GO_SERVER_METRICS_PORT=9090
RUST_SERVER_PORT=50052
RUST_SERVER_METRICS_PORT=9092

# Prometheus
PROMETHEUS_PORT=9093

# Grafana
GRAFANA_PORT=3000
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=admin

# Client
APISIX_ADDR=apisix:9080
