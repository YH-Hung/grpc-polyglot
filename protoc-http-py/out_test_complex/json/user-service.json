{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://example.com/schemas/user-service.json",
  "title": "Schemas for user-service.proto",
  "description": "JSON Schema definitions for all messages and enums in user-service.proto (package: user)",
  "$defs": {
    "TradeAction": {
      "type": "string",
      "enum": [
        "BUY",
        "SELL"
      ],
      "description": "Enum values: BUY=0, SELL=1"
    },
    "UserInformationRequest": {
      "type": "object",
      "properties": {
        "userId": {
          "type": "integer",
          "format": "int32"
        }
      },
      "additionalProperties": false
    },
    "UserInformation": {
      "type": "object",
      "properties": {
        "userId": {
          "type": "integer",
          "format": "int32"
        },
        "name": {
          "type": "string"
        },
        "balance": {
          "type": "integer",
          "format": "int32"
        },
        "holdings": {
          "type": "array",
          "items": {
            "$ref": "#/$defs/Holding"
          }
        }
      },
      "additionalProperties": false
    },
    "Holding": {
      "type": "object",
      "properties": {
        "ticker": {
          "$ref": "common.json#/$defs/Ticker"
        },
        "quantity": {
          "type": "integer",
          "format": "int32"
        }
      },
      "additionalProperties": false
    },
    "StockTradeRequest": {
      "type": "object",
      "properties": {
        "userId": {
          "type": "integer",
          "format": "int32"
        },
        "ticker": {
          "$ref": "common.json#/$defs/Ticker"
        },
        "price": {
          "type": "integer",
          "format": "int32"
        },
        "quantity": {
          "type": "integer",
          "format": "int32"
        },
        "action": {
          "$ref": "#/$defs/TradeAction"
        }
      },
      "additionalProperties": false
    },
    "StockTradeResponse": {
      "type": "object",
      "properties": {
        "userId": {
          "type": "integer",
          "format": "int32"
        },
        "ticker": {
          "$ref": "common.json#/$defs/Ticker"
        },
        "price": {
          "type": "integer",
          "format": "int32"
        },
        "quantity": {
          "type": "integer",
          "format": "int32"
        },
        "action": {
          "$ref": "#/$defs/TradeAction"
        },
        "totalPrice": {
          "type": "integer",
          "format": "int32"
        },
        "balance": {
          "type": "integer",
          "format": "int32"
        }
      },
      "additionalProperties": false
    }
  }
}